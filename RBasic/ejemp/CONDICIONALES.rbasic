R CONDICION ES BOOLEANO
CONDICION = VERDADERO
SI CONDICION EQUIVALE VERDADERO ENTONCES
	IMPRIME "ENTRA PRIMER CONDICION"
	R A ES ENTERO
	R B ES ENTERO
	A = 2
	B = 1
	SI A MENORQUE B ENTONCES
		IMPRIME "ENTRA SEGUNDA CONDICION"
	SINO A MAYORQUE B ENTONCES 
		IMPRIME "ENTRA A TERCERA CONDICION"
	NO
		IMPRIME "ENTRA A CONDICION SI NO ES NINGUA DE LAS ANTERIORES"
	TSI
	A = 3
TSI