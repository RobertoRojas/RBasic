R A ES BOOLEANO
A = VERDADERO
R I ES ENTERO
I = 0
R TOPE ES ENTERO
TOPE = 6
IMPRIME "ENTRA A CICLOS"
MIENTRAS A ENTONCES
	I = I + 1
	R B ES BOOLEANO
	B = VERDADERO
	R J ES ENTERO
	J = I
	IMPRIME "I: " + I
	MIENTRAS B Y J MENORQUE TOPE ENTONCES
		J = J + 1
		IMPRIME "J: " + J
		SI J MAYOROIGUALQUE TOPE ENTONCES
			B = FALSO
		TSI
	TMIENTRAS
	SI I MAYOROIGUALQUE TOPE ENTONCES
		A = FALSO
	TSI
TMIENTRAS
IMPRIME "SALE DE CICLOS"
